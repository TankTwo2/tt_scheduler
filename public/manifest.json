{
  "name": "tt_scheduler",
  "description": "Override the new tab page with a blank one",
  "version": "1.0.1",
  "incognito": "split",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Open the popup"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage", "identity", "identity.email"],
  "icons": {
    "128": "icon.png"
  },
  "content_security_policy": "script-src 'self' https://use.fontawesome.com; object-src 'self'",
  "manifest_version": 2
}
