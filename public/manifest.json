{
	"name": "tt_scheduler",
	"description": "Override the new tab page with a blank one",
	"version": "0.2.4",
	"incognito": "split",
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Open the popup"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": [
		"storage"
	],
	"manifest_version": 2
}